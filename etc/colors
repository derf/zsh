## Colors
## vim:ft=zsh
local fail=$'\e[1;31m'
local warn=$'\e[1;33m'
local info=$'\e[0;36m'
local yellow=$'\e[0;33m'
local green=$'\e[0;32m'
local red=$'\e[0;31m'
local reset=$'\e[0m'

local ps_fail="%{$fail%}"
local ps_info="%{$info%}"
local ps_yellow="%{$yellow%}"
local ps_green="%{$green%}"
local ps_red="%{$red%}"
local ps_reset="%{$reset%}"
